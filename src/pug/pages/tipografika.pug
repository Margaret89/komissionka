extends ../layout/main

block variables
	- var title = 'Типографика';
	- var h1 = 'Типографика';


block breadcrumb
	.container
		.row
			.col-12
				-
					var breadcrumbs=[
						{href:'/',name:'Главная'},
						{href:'#',name:'Типографика'},
					];

				+breadcrumbs(breadcrumbs)

block content
